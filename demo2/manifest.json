{
  "name": "block/allow third-party cookies api",
  "version": "1",
  "manifest_version": 3,
  "description": "展示扩展如何控制 Cookie 的接受与否",
  "permissions": [
    "privacy"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "./icons/32.png"
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "64": "./icons/64.png",
    "128": "./icons/128.png"
  }
}