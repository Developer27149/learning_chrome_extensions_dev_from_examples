{
  "name": "谷歌事件追踪分析",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A sample extension which uses Google Analytics to track usage.",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "64": "./icons/64.png",
    "128": "./icons/128.png"
  },
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "permissions": []
}